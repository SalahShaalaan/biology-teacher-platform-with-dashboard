{
    "version": 2,
    "builds": [
      {
        "src": "server/src/index.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["server/public/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/src/index.ts"
      }
    ]
  }